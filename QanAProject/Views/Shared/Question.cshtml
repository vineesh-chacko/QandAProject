@model QandAViewModels.QuestionViewModel
<tr>
    <td><span class="badge badge-secondary">@Model.VotesCount</span></td>
    <td><span class="badge badge-secondary">@Model.AnswersCount</span></td>
    <td><span class="badge badge-secondary">@Model.ViewsCount</span></td>
    <td><span class="badge badge-secondary">@Model.ViewsCount</span></td>
    <td class="text-dark">
        <a href="questions/view/@Model.QuestionId" class="text-primary">@Model.QuestionName</a>
        <span class="badge badge-info">@Model.Category.CategoryName</span>
    </td>
    <td>
        @if (Convert.ToInt32(Session["CurrentUserId"]) != Model.UserID)
        {
            @:by @Model.User.Name
        }
        else
        {
            @:by you.
        }
    </td>
    <td>on @Model.QuestionDateAndTime</td>

</tr>